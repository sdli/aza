{
  "name": "aza",
  "version": "1.0.27",
  "description": "A redux/react-redux/redux-immutable/react-router packs for building store and routers in react-apps.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-effects": "npx babel ./src/effects --out-dir effects",
    "build-router": "npx babel ./src/routers --out-dir router",
    "build-lib": "npx babel ./src --out-dir lib",
    "build-immutable": "npx babel ./src/immutable --out-dir immutable",
    "build": "npm run build-effects && npm run build-immutable && npm run build-router && npm run build-lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sdli/aza.git"
  },
  "keywords": [
    "redux",
    "react",
    "redux-immutable",
    "immutable",
    "react"
  ],
  "peerDependencies": {
    "react": "^16.0.0-0",
    "react-dom": "^16.0.0-0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
